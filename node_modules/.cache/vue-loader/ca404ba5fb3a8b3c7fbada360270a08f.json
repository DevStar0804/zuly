A;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Store.vue","sourceRoot":"src/pages","sourcesContent":["<template>\r\n  <div class=\"container mt-3\">\r\n    <small class=\"text-muted\">{{ $options.__file }}</small>\r\n    <h1 class=\"text-primary font-weight-bold\">\r\n      <router-link to=\"/\"\r\n        ><img\r\n          src=\"~bootstrap-icons/icons/caret-left-fill.svg\"\r\n          alt=\"caret icon\"\r\n          width=\"30\"\r\n          height=\"30\"\r\n      /></router-link>\r\n      Vetrina digitale di: {{ store.company_name }}\r\n    </h1>\r\n    <hr />\r\n    <!-- <div class=\"row alert-danger\">TODO</div> -->\r\n    <div class=\"row m-3\">\r\n      <div class=\"col\">\r\n        <pre>{{ store }}</pre>\r\n        <div\r\n          id=\"carouselExampleCaptions\"\r\n          class=\"carousel slide\"\r\n          data-ride=\"carousel\"\r\n        >\r\n          <ol class=\"carousel-indicators\">\r\n            <li\r\n              v-for=\"indicator in imgindicators\"\r\n              :key=\"indicator.slide\"\r\n              data-target=\"#carouselExampleCaptions\"\r\n              :data-slide-to=\"indicator.slide\"\r\n              :class=\"indicator.classname\"\r\n            ></li>\r\n          </ol>\r\n          <div class=\"carousel-inner\">\r\n            <div\r\n              :class=\"imgurl.classname\"\r\n              v-for=\"imgurl in imgurls\"\r\n              :key=\"imgurl.url\"\r\n            >\r\n              <img\r\n                :src=\"imgurl.url\"\r\n                class=\"rounded mx-auto d-block w-50\"\r\n                alt=\"Image\"\r\n              />\r\n            </div>\r\n          </div>\r\n          <a\r\n            class=\"carousel-control-prev\"\r\n            href=\"#carouselExampleCaptions\"\r\n            role=\"button\"\r\n            data-slide=\"prev\"\r\n          >\r\n            <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\r\n            <span class=\"visually-hidden\">Previous</span>\r\n          </a>\r\n          <a\r\n            class=\"carousel-control-next\"\r\n            href=\"#carouselExampleCaptions\"\r\n            role=\"button\"\r\n            data-slide=\"next\"\r\n          >\r\n            <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\r\n            <span class=\"visually-hidden\">Next</span>\r\n          </a>\r\n        </div>\r\n\r\n        <div>\r\n          <label for=\"description\" style=\"font-size: 24px\">Description</label>\r\n          <div class=\"editor\">\r\n            <editor-menu-bubble\r\n              class=\"menububble\"\r\n              :editor=\"editor\"\r\n              @hide=\"hideLinkMenu\"\r\n              v-slot=\"{ commands, isActive, getMarkAttrs, menu }\"\r\n            >\r\n              <div\r\n                class=\"menububble\"\r\n                :class=\"{ 'is-active': menu.isActive }\"\r\n                :style=\"`left: ${menu.left}px; bottom: ${menu.bottom}px;`\"\r\n              >\r\n                <form\r\n                  class=\"menububble__form\"\r\n                  v-if=\"linkMenuIsActive\"\r\n                  @submit.prevent=\"setLinkUrl(commands.link, linkUrl)\"\r\n                >\r\n                  <input\r\n                    class=\"menububble__input\"\r\n                    type=\"text\"\r\n                    v-model=\"linkUrl\"\r\n                    placeholder=\"https://\"\r\n                    ref=\"linkInput\"\r\n                    @keydown.esc=\"hideLinkMenu\"\r\n                  />\r\n                  <button\r\n                    class=\"menububble__button\"\r\n                    @click=\"setLinkUrl(commands.link, null)\"\r\n                    type=\"button\"\r\n                  >\r\n                    <icon name=\"remove\" />\r\n                  </button>\r\n                </form>\r\n\r\n                <template v-else>\r\n                  <button\r\n                    class=\"menububble__button\"\r\n                    @click=\"showLinkMenu(getMarkAttrs('link'))\"\r\n                    :class=\"{ 'is-active': isActive.link() }\"\r\n                  >\r\n                    <span>{{\r\n                      isActive.link() ? \"Update Link\" : \"Add Link\"\r\n                    }}</span>\r\n                    <icon name=\"link\" />\r\n                  </button>\r\n                </template>\r\n              </div>\r\n            </editor-menu-bubble>\r\n\r\n            <!-- <editor-menu-bar :editor=\"editor\" v-slot=\"{ commands, isActive }\">\r\n              <div class=\"menubar\">\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.bold() }\"\r\n                  @click=\"commands.bold\"\r\n                >\r\n                  <icon name=\"bold\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.italic() }\"\r\n                  @click=\"commands.italic\"\r\n                >\r\n                  <icon name=\"italic\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.strike() }\"\r\n                  @click=\"commands.strike\"\r\n                >\r\n                  <icon name=\"strike\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.underline() }\"\r\n                  @click=\"commands.underline\"\r\n                >\r\n                  <icon name=\"underline\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.code() }\"\r\n                  @click=\"commands.code\"\r\n                >\r\n                  <icon name=\"code\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.paragraph() }\"\r\n                  @click=\"commands.paragraph\"\r\n                >\r\n                  <icon name=\"paragraph\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.heading({ level: 1 }) }\"\r\n                  @click=\"commands.heading({ level: 1 })\"\r\n                >\r\n                  H1\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.heading({ level: 2 }) }\"\r\n                  @click=\"commands.heading({ level: 2 })\"\r\n                >\r\n                  H2\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.heading({ level: 3 }) }\"\r\n                  @click=\"commands.heading({ level: 3 })\"\r\n                >\r\n                  H3\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.bullet_list() }\"\r\n                  @click=\"commands.bullet_list\"\r\n                >\r\n                  <icon name=\"ul\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.ordered_list() }\"\r\n                  @click=\"commands.ordered_list\"\r\n                >\r\n                  <icon name=\"ol\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.blockquote() }\"\r\n                  @click=\"commands.blockquote\"\r\n                >\r\n                  <icon name=\"quote\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  :class=\"{ 'is-active': isActive.code_block() }\"\r\n                  @click=\"commands.code_block\"\r\n                >\r\n                  <icon name=\"code\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  @click=\"commands.horizontal_rule\"\r\n                >\r\n                  <icon name=\"hr\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  @click=\"commands.undo\"\r\n                >\r\n                  <icon name=\"undo\" />\r\n                </button>\r\n\r\n                <button\r\n                  class=\"menubar__button\"\r\n                  @click=\"commands.redo\"\r\n                >\r\n                  <icon name=\"redo\" />\r\n                </button>\r\n\r\n              </div>\r\n            </editor-menu-bar> -->\r\n            <editor-content\r\n              class=\"editor__content\"\r\n              :editor=\"editor\"\r\n              id=\"description\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <div class=\"row\">\r\n            <div class=\"col-12 col-md-8 col-sm-8\">\r\n              <label for=\"CurrentCategories\" style=\"font-size: 24px\"\r\n                >Categories</label\r\n              >\r\n              <select\r\n                class=\"form-select\"\r\n                aria-label=\"Default select example\"\r\n                v-model=\"currentcategory\"\r\n                v-on:change=\"onCategoryChange(currentcategory)\"\r\n                id=\"CurrentCategories\"\r\n              >\r\n                <option\r\n                  v-for=\"option in CurrentCategories\"\r\n                  :key=\"option.key\"\r\n                  :value=\"option.value\"\r\n                >\r\n                  {{ option.text }}\r\n                </option>\r\n              </select>\r\n              <div class=\"row\">\r\n                <div class=\"col-12 col-md-4 col-sm-4\">\r\n                  <label for=\"AddCategories\" style=\"font-size: 24px\"\r\n                    >Add</label\r\n                  >\r\n                  <select\r\n                    class=\"form-select\"\r\n                    aria-label=\"Default select example\"\r\n                    v-model.lazy=\"addcategory\"\r\n                    id=\"AddCategories\"\r\n                  >\r\n                    <option\r\n                      v-for=\"option in Categories\"\r\n                      :key=\"option.id\"\r\n                      :value=\"option.id\"\r\n                    >\r\n                      {{ option.display }}\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n                <div class=\"col-12 col-md-4 col-sm-4\">\r\n                  <label for=\"MainCategories\" style=\"font-size: 24px\"\r\n                    >Main</label\r\n                  >\r\n                  <select\r\n                    class=\"form-select\"\r\n                    aria-label=\"Default select example\"\r\n                    v-model.lazy=\"maincategory\"\r\n                    id=\"MainCategories\"\r\n                  >\r\n                    <option\r\n                      v-for=\"option in CurrentCategories\"\r\n                      :key=\"option.key\"\r\n                      :value=\"option.value\"\r\n                    >\r\n                      {{ option.text }}\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n                <div class=\"col-12 col-md-4 col-sm-4\">\r\n                  <label for=\"DeleteCategories\" style=\"font-size: 24px\"\r\n                    >Delete</label\r\n                  >\r\n                  <select\r\n                    class=\"form-select\"\r\n                    aria-label=\"Default select example\"\r\n                    v-model.lazy=\"deletecategory\"\r\n                    id=\"DeleteCategories\"\r\n                  >\r\n                    <option\r\n                      v-for=\"option in CurrentCategories\"\r\n                      :key=\"option.key\"\r\n                      :value=\"option.value\"\r\n                    >\r\n                      {{ option.text }}\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-12 col-md-4 col-sm-4\">\r\n              <img\r\n                :src=\"categoryimg\"\r\n                :alt=\"currentcategory\"\r\n                v-if=\"categoryimg\"\r\n                width=\"200\"\r\n                height=\"200\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <location-picker v-model=\"location\" :options=\"options\">\r\n          </location-picker>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Vue from \"vue\";\r\nVue.use(LocationPicker, {\r\n  installComponents: false, // If true, create it globally\r\n});\r\nimport Icon from \"../components/Icon\";\r\nimport { Editor, EditorContent, EditorMenuBubble, EditorMenuBar } from \"tiptap\";\r\nimport {\r\n  Blockquote,\r\n  BulletList,\r\n  CodeBlock,\r\n  HardBreak,\r\n  Heading,\r\n  HorizontalRule,\r\n  ListItem,\r\n  OrderedList,\r\n  TodoItem,\r\n  TodoList,\r\n  Bold,\r\n  Code,\r\n  Italic,\r\n  Link,\r\n  Strike,\r\n  Underline,\r\n  History,\r\n} from \"tiptap-extensions\";\r\n// import * as LocationPicker from 'vue2-location-picker'\r\nimport { LocationPicker } from \"vue2-location-picker\";\r\nexport default {\r\n  components: {\r\n    EditorContent,\r\n    EditorMenuBubble,\r\n    EditorMenuBar,\r\n    Icon,\r\n    LocationPicker,\r\n  },\r\n  data() {\r\n    return {\r\n      store_id: null,\r\n      store: {},\r\n      editor: new Editor({\r\n        extensions: [\r\n          new Blockquote(),\r\n          new BulletList(),\r\n          new CodeBlock(),\r\n          new HardBreak(),\r\n          new Heading({ levels: [1, 2, 3] }),\r\n          new HorizontalRule(),\r\n          new ListItem(),\r\n          new OrderedList(),\r\n          new TodoItem(),\r\n          new TodoList(),\r\n          new Link(),\r\n          new Bold(),\r\n          new Code(),\r\n          new Italic(),\r\n          new Strike(),\r\n          new Underline(),\r\n          new History(),\r\n        ],\r\n        content: \"\",\r\n      }),\r\n      linkUrl: null,\r\n      linkMenuIsActive: false,\r\n      CurrentCategories: [],\r\n      Categories: [],\r\n      currentcategory: \"\",\r\n      addcategory: \"\",\r\n      maincategory: \"\",\r\n      deletecategory: \"\",\r\n      categoryimg: \"\",\r\n      imgurls: [],\r\n      imgindicators: [],\r\n      location: {\r\n        lat: 0,\r\n        lng: 0,\r\n      },\r\n      options: {\r\n        // is not required\r\n        map: {\r\n          /** other map options **/\r\n        },\r\n        marker: {\r\n          /** marker options **/\r\n        },\r\n        autocomplete: {\r\n          /** autocomplete options **/\r\n        },\r\n      },\r\n    };\r\n  },\r\n  beforeDestroy() {\r\n    this.editor.destroy();\r\n  },\r\n  watch: {\r\n    addcategory: function () {\r\n      this.AddCategory().then(data=>{\r\n        if(data=\"ok\"){\r\n          this.Categories.forEach(category => {\r\n            if(category.id==this.addcategory)\r\n              alert(\"Aggiunti i \"+category.display+\" con successo!\")\r\n          });\r\n        }\r\n      })\r\n    },\r\n    maincategory: function () {\r\n      this.MainCategory().then(data=>{\r\n        if(data=\"ok\"){\r\n          this.Categories.forEach(category => {\r\n            if(category.id==this.maincategory)\r\n              alert(\"Selezionato con successo i \"+category.display+\" come categoria principale!\")\r\n          });\r\n        }\r\n      })\r\n    },\r\n    deletecategory: function () {\r\n      if(confirm(\"Vuoi rimuovere questa categoria?\")){\r\n        this.DeleteCategory().then(data=>{\r\n          console.log(data)\r\n          if(data=\"ok\"){\r\n            this.Categories.forEach(category => {\r\n              if(category.id==this.deletecategory)\r\n                alert(\"Eliminato con successo i \"+category.display+\"!\")\r\n            });\r\n          }\r\n        })\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchStore(storeId) {\r\n      const response = await fetch(\"/api/stores/\" + storeId, {\r\n        mode: \"no-cors\",\r\n        headers: {\r\n          accept: \"application/json\",\r\n        },\r\n      });\r\n      return response.json();\r\n    },\r\n    async fetchCategory() {\r\n      const response = await fetch(\"/api/categories\", {\r\n        mode: \"no-cors\",\r\n        headers: {\r\n          accept: \"application/json\",\r\n        },\r\n      });\r\n      return response.json();\r\n    },\r\n    async AddCategory() {\r\n      const response = await fetch(\"/api/stores/\"+this.store_id+\"/categories/\"+this.addcategory,{\r\n        method:'POST',\r\n        mode: 'no-cors',\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n      })\r\n      return response.text()\r\n    },\r\n    async MainCategory() {\r\n      const response = await fetch(\"/api/stores/\"+this.store_id+\"/categories/\"+this.maincategory+\"/main\",{\r\n        method:'POST',\r\n        mode: 'no-cors',\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n      })\r\n      return response.text()\r\n    },\r\n    async DeleteCategory() {\r\n      const response = await fetch(\"/api/stores/\"+this.store_id+\"/categories/\"+this.maincategory,{\r\n        method:'DELETE',\r\n        mode: 'no-cors',\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n      })\r\n      return response.text()\r\n    },\r\n    showLinkMenu(attrs) {\r\n      this.linkUrl = attrs.href;\r\n      this.linkMenuIsActive = true;\r\n      this.$nextTick(() => {\r\n        this.$refs.linkInput.focus();\r\n      });\r\n    },\r\n    hideLinkMenu() {\r\n      this.linkUrl = null;\r\n      this.linkMenuIsActive = false;\r\n    },\r\n    setLinkUrl(command, url) {\r\n      command({ href: url });\r\n      this.hideLinkMenu();\r\n    },\r\n    onCategoryChange(currentcategory) {\r\n      this.Categories.forEach((category) => {\r\n        if (category.id == currentcategory) {\r\n          this.categoryimg = category.image;\r\n        }\r\n      });\r\n    },\r\n  },\r\n  mounted: function () {\r\n    this.store_id = this.$route.params.id;\r\n    this.fetchStore(this.store_id).then((data) => {\r\n      this.store = data;\r\n      this.editor.setContent(data.description);\r\n      this.location.lat = data.latitude;\r\n      this.location.lng = data.longitude;\r\n      for (let category in data.categories) {\r\n        this.CurrentCategories.push({\r\n          key: category,\r\n          text: category,\r\n          value: data.categories[category],\r\n        });\r\n      }\r\n      for (let url in data.images) {\r\n        let imgurl =\r\n          \"https://my.zuly.it/api/stores/\" +\r\n          this.$route.params.id +\r\n          \"/images/\" +\r\n          data.images[url];\r\n        if (data.images[url] != null) {\r\n          if (url == 0) {\r\n            this.imgurls.push({\r\n              url: imgurl,\r\n              classname: \"carousel-item active\",\r\n            });\r\n            this.imgindicators.push({\r\n              slide: url,\r\n              classname: \"active\",\r\n            });\r\n          } else {\r\n            this.imgurls.push({\r\n              url: imgurl,\r\n              classname: \"carousel-item\",\r\n            });\r\n            this.imgindicators.push({\r\n              slide: url,\r\n              classname: \"\",\r\n            });\r\n          }\r\n        }\r\n      }\r\n    });\r\n    this.fetchCategory().then((data) => {\r\n      this.Categories = data;\r\n    });\r\n  },\r\n};\r\n</script>"]}]}