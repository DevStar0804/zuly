AA,EAXA,EAYA,IAAA,IAAA,EAZA,EAaA,IAAA,IAAA,EAbA,EAcA,IAAA,MAAA,EAdA,EAeA,IAAA,MAAA,EAfA,EAgBA,IAAA,SAAA,EAhBA,EAiBA,IAAA,OAAA,EAjBA,CADA;AAoBA,QAAA,OAAA,EAAA;AApBA,OAAA,CAHA;AAyBA,MAAA,OAAA,EAAA,IAzBA;AA0BA,MAAA,gBAAA,EAAA,KA1BA;AA2BA,MAAA,eAAA,EAAA,EA3BA;AA4BA,MAAA,UAAA,EAAA,EA5BA;AA6BA,MAAA,gBAAA,EAAA,EA7BA;AA8BA,MAAA,WAAA,EAAA,EA9BA;AA+BA,MAAA,YAAA,EAAA,EA/BA;AAgCA,MAAA,OAAA,EAAA;AAhCA,KAAA;AAkCA,GA1CA;;AA2CA,EAAA,aAAA,GAAA;AACA,SAAA,MAAA,CAAA,OAAA;AACA,GA7CA;;AA8CA,EAAA,OAAA,EAAA;AACA,UAAA,UAAA,CAAA,OAAA,EAAA;AACA,YAAA,QAAA,GAAA,MAAA,KAAA,CAAA,iBAAA,OAAA,EAAA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,OAAA,EAAA;AACA,oBAAA;AADA;AAFA,OAAA,CAAA;AAOA,aAAA,QAAA,CAAA,IAAA,EAAA;AACA,KAVA;;AAWA,UAAA,aAAA,GAAA;AACA,YAAA,YAAA,GAAA,MAAA,KAAA,CAAA,iBAAA,EAAA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,OAAA,EAAA;AACA,oBAAA;AADA;AAFA,OAAA,CAAA;AAMA,aAAA,YAAA,CAAA,IAAA,EAAA;AACA,KAnBA;;AAoBA,IAAA,YAAA,CAAA,KAAA,EAAA;AACA,WAAA,OAAA,GAAA,KAAA,CAAA,IAAA;AACA,WAAA,gBAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,MAAA;AACA,aAAA,KAAA,CAAA,SAAA,CAAA,KAAA;AACA,OAFA;AAGA,KA1BA;;AA2BA,IAAA,YAAA,GAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,gBAAA,GAAA,KAAA;AACA,KA9BA;;AA+BA,IAAA,UAAA,CAAA,OAAA,EAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA,CAAA;AACA,WAAA,YAAA;AACA,KAlCA;;AAmCA,IAAA,gBAAA,CAAA,gBAAA,EAAA;AACA,WAAA,UAAA,CAAA,OAAA,CAAA,QAAA,IAAA;AACA,YAAA,QAAA,CAAA,EAAA,IAAA,gBAAA,EAAA;AACA,eAAA,WAAA,GAAA,QAAA,CAAA,KAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,KAAA;AACA;AACA,OALA;AAMA;;AA1CA,GA9CA;AA0FA,EAAA,OAAA,EAAA,YAAA;AACA,SAAA,QAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,SAAA,UAAA,CAAA,KAAA,QAAA,EAAA,IAAA,CAAA,IAAA,IAAA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,WAAA,MAAA,CAAA,UAAA,CAAA,IAAA,CAAA,WAAA;;AACA,WAAA,IAAA,QAAA,IAAA,IAAA,CAAA,UAAA,EAAA;AACA,aAAA,eAAA,CAAA,IAAA,CAAA;AACA,UAAA,GAAA,EAAA,QADA;AAEA,UAAA,IAAA,EAAA,QAFA;AAGA,UAAA,KAAA,EAAA,IAAA,CAAA,UAAA,CAAA,QAAA;AAHA,SAAA;AAKA;;AACA,WAAA,YAAA,GAAA,IAAA,CAAA,aAAA,CAAA,OAAA;;AACA,WAAA,IAAA,GAAA,IAAA,IAAA,CAAA,MAAA,EAAA;AACA,YAAA,MAAA,GAAA,mCAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,GAAA,UAAA,GAAA,IAAA,CAAA,MAAA,CAAA,GAAA,CAAA;;AACA,YAAA,IAAA,CAAA,MAAA,CAAA,GAAA,KAAA,IAAA,EAAA;AACA,cAAA,GAAA,IAAA,CAAA,EACA,KAAA,OAAA,CAAA,IAAA,CAAA;AACA,YAAA,GAAA,EAAA,MADA;AAEA,YAAA,SAAA,EAAA;AAFA,WAAA,EADA,KAMA,KAAA,OAAA,CAAA,IAAA,CAAA;AACA,YAAA,GAAA,EAAA,MADA;AAEA,YAAA,SAAA,EAAA;AAFA,WAAA;AAIA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA;AACA,KA3BA;AA4BA,SAAA,aAAA,GAAA,IAAA,CAAA,IAAA,IAAA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KAFA;AAGA;AA3HA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container mt-3\">\r\n    <small class=\"text-muted\">{{ $options.__file }}</small>\r\n    <h1 class=\"text-primary font-weight-bold\">\r\n      <router-link to=\"/\"\r\n        ><img\r\n          src=\"~bootstrap-icons/icons/caret-left-fill.svg\"\r\n          alt=\"caret icon\"\r\n          width=\"30\"\r\n          height=\"30\"\r\n      /></router-link>\r\n      Vetrina digitale di: {{ store.company_name }}\r\n    </h1>\r\n    <hr />\r\n    <!-- <div class=\"row alert-danger\">TODO</div> -->\r\n    <div class=\"row m-3\">\r\n      <div class=\"col\">\r\n        <!-- <pre>{{ store }}</pre> -->\r\n        <div id=\"carouselExampleCaptions\" class=\"carousel slide\" data-ride=\"carousel\">\r\n          <ol class=\"carousel-indicators\">\r\n            <li data-target=\"#carouselExampleCaptions\" data-slide-to=\"0\" class=\"active\"></li>\r\n            <li data-target=\"#carouselExampleCaptions\" data-slide-to=\"1\"></li>\r\n            <li data-target=\"#carouselExampleCaptions\" data-slide-to=\"2\"></li>\r\n          </ol>\r\n          <div class=\"carousel-inner\">\r\n            <div :class=\"imgurl.classname\" v-for=\"imgurl in imgurls\" :key=\"imgurl.url\">\r\n              <img :src=\"imgurl.url\" class=\"d-block w-50 col-md-6 offset-md-3\" alt=\"Image\" height=\"300\">\r\n            </div>\r\n          </div>\r\n          <a class=\"carousel-control-prev\" href=\"#carouselExampleCaptions\" role=\"button\" data-slide=\"prev\">\r\n            <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\r\n            <span class=\"visually-hidden\">Previous</span>\r\n          </a>\r\n          <a class=\"carousel-control-next\" href=\"#carouselExampleCaptions\" role=\"button\" data-slide=\"next\">\r\n            <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\r\n            <span class=\"visually-hidden\">Next</span>\r\n          </a>\r\n        </div>\r\n        <label for=\"description\" style=\"font-size:24px;\">Description</label>\r\n        <div class=\"editor\">\r\n          <editor-menu-bubble\r\n            class=\"menububble\"\r\n            :editor=\"editor\"\r\n            @hide=\"hideLinkMenu\"\r\n            v-slot=\"{ commands, isActive, getMarkAttrs, menu }\"\r\n          >\r\n            <div\r\n              class=\"menububble\"\r\n              :class=\"{ 'is-active': menu.isActive }\"\r\n              :style=\"`left: ${menu.left}px; bottom: ${menu.bottom}px;`\"\r\n            >\r\n              <form\r\n                class=\"menububble__form\"\r\n                v-if=\"linkMenuIsActive\"\r\n                @submit.prevent=\"setLinkUrl(commands.link, linkUrl)\"\r\n              >\r\n                <input\r\n                  class=\"menububble__input\"\r\n                  type=\"text\"\r\n                  v-model=\"linkUrl\"\r\n                  placeholder=\"https://\"\r\n                  ref=\"linkInput\"\r\n                  @keydown.esc=\"hideLinkMenu\"\r\n                />\r\n                <button\r\n                  class=\"menububble__button\"\r\n                  @click=\"setLinkUrl(commands.link, null)\"\r\n                  type=\"button\"\r\n                >\r\n                  <icon name=\"remove\" />\r\n                </button>\r\n              </form>\r\n\r\n              <template v-else>\r\n                <button\r\n                  class=\"menububble__button\"\r\n                  @click=\"showLinkMenu(getMarkAttrs('link'))\"\r\n                  :class=\"{ 'is-active': isActive.link() }\"\r\n                >\r\n                  <span>{{\r\n                    isActive.link() ? \"Update Link\" : \"Add Link\"\r\n                  }}</span>\r\n                  <icon name=\"link\" />\r\n                </button>\r\n              </template>\r\n            </div>\r\n          </editor-menu-bubble>\r\n\r\n          <!-- <editor-menu-bar :editor=\"editor\" v-slot=\"{ commands, isActive }\">\r\n            <div class=\"menubar\">\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.bold() }\"\r\n                @click=\"commands.bold\"\r\n              >\r\n                <icon name=\"bold\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.italic() }\"\r\n                @click=\"commands.italic\"\r\n              >\r\n                <icon name=\"italic\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.strike() }\"\r\n                @click=\"commands.strike\"\r\n              >\r\n                <icon name=\"strike\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.underline() }\"\r\n                @click=\"commands.underline\"\r\n              >\r\n                <icon name=\"underline\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.code() }\"\r\n                @click=\"commands.code\"\r\n              >\r\n                <icon name=\"code\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.paragraph() }\"\r\n                @click=\"commands.paragraph\"\r\n              >\r\n                <icon name=\"paragraph\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.heading({ level: 1 }) }\"\r\n                @click=\"commands.heading({ level: 1 })\"\r\n              >\r\n                H1\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.heading({ level: 2 }) }\"\r\n                @click=\"commands.heading({ level: 2 })\"\r\n              >\r\n                H2\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.heading({ level: 3 }) }\"\r\n                @click=\"commands.heading({ level: 3 })\"\r\n              >\r\n                H3\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.bullet_list() }\"\r\n                @click=\"commands.bullet_list\"\r\n              >\r\n                <icon name=\"ul\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.ordered_list() }\"\r\n                @click=\"commands.ordered_list\"\r\n              >\r\n                <icon name=\"ol\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.blockquote() }\"\r\n                @click=\"commands.blockquote\"\r\n              >\r\n                <icon name=\"quote\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                :class=\"{ 'is-active': isActive.code_block() }\"\r\n                @click=\"commands.code_block\"\r\n              >\r\n                <icon name=\"code\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                @click=\"commands.horizontal_rule\"\r\n              >\r\n                <icon name=\"hr\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                @click=\"commands.undo\"\r\n              >\r\n                <icon name=\"undo\" />\r\n              </button>\r\n\r\n              <button\r\n                class=\"menubar__button\"\r\n                @click=\"commands.redo\"\r\n              >\r\n                <icon name=\"redo\" />\r\n              </button>\r\n\r\n            </div>\r\n          </editor-menu-bar> -->\r\n          <editor-content class=\"editor__content\" :editor=\"editor\" id=\"description\" />\r\n        </div>\r\n        <label for=\"Categories\" style=\"font-size:24px;\">Categories</label>\r\n        <div class=\"row\">\r\n          <div class=\"col-12 col-md-8 col-sm-8\" >\r\n            <select\r\n              class=\"form-select\"\r\n              aria-label=\"Default select example\"\r\n              v-model=\"selectedcategory\"\r\n              v-on:change=\"onCategoryChange(selectedcategory)\"\r\n              id=\"categories\"\r\n            >\r\n              <option\r\n                v-for=\"option in categoryoptions\"\r\n                :key=\"option.key\"\r\n                :value=\"option.value\"\r\n              >\r\n                {{ option.text }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n          <div class=\"col-12 col-md-4 col-sm-4\">\r\n            <img :src=\"categoryimg\" :alt=\"selectedcategory\" v-if=\"categoryimg\" width=\"200\" height=\"200\">\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <label for=\"maincategory\" style=\"font-size:24px;\">Main Category:{{maincategory}}</label>\r\n        </div>\r\n        <div>\r\n          <label>Insert a Geo Location \r\n            <input id=\"geolocs\" type=\"text\" value=\"\" />\r\n          </label>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Icon from '../components/Icon'\r\nimport { Editor, EditorContent, EditorMenuBubble, EditorMenuBar } from 'tiptap'\r\nimport {\r\n  Blockquote,\r\n  BulletList,\r\n  CodeBlock,\r\n  HardBreak,\r\n  Heading,\r\n  HorizontalRule,\r\n  ListItem,\r\n  OrderedList,\r\n  TodoItem,\r\n  TodoList,\r\n  Bold,\r\n  Code,\r\n  Italic,\r\n  Link,\r\n  Strike,\r\n  Underline,\r\n  History,\r\n} from 'tiptap-extensions'\r\nimport  leafletLocationPicker from '../../public/leaflet-locationpicker'\r\nexport default {\r\n  components: {\r\n    EditorContent,\r\n    EditorMenuBubble,\r\n    EditorMenuBar,\r\n    Icon,\r\n  },\r\n  data () {\r\n    return {\r\n      store_id: null,\r\n      store: {},\r\n      editor: new Editor({\r\n        extensions: [\r\n          new Blockquote(),\r\n          new BulletList(),\r\n          new CodeBlock(),\r\n          new HardBreak(),\r\n          new Heading({ levels: [1, 2, 3] }),\r\n          new HorizontalRule(),\r\n          new ListItem(),\r\n          new OrderedList(),\r\n          new TodoItem(),\r\n          new TodoList(),\r\n          new Link(),\r\n          new Bold(),\r\n          new Code(),\r\n          new Italic(),\r\n          new Strike(),\r\n          new Underline(),\r\n          new History(),\r\n        ],\r\n        content: '',\r\n      }),\r\n      linkUrl: null,\r\n      linkMenuIsActive: false,\r\n      categoryoptions:[],\r\n      categories:[],\r\n      selectedcategory:'',\r\n      categoryimg:'',\r\n      maincategory:'',\r\n      imgurls:[],\r\n    }\r\n  },\r\n  beforeDestroy() {\r\n    this.editor.destroy()\r\n  },\r\n  methods: {\r\n    async fetchStore (storeId) {\r\n      const response = await fetch('/api/stores/' + storeId, {\r\n        mode: 'no-cors',\r\n        headers: {\r\n          'accept': 'application/json'\r\n        }\r\n      }\r\n      )\r\n      return response.json()\r\n    },\r\n    async fetchCategory(){\r\n      const categorydata = await fetch('/api/categories', {\r\n        mode: 'no-cors',\r\n        headers: {\r\n          'accept': 'application/json'\r\n        }\r\n      })\r\n      return categorydata.json()\r\n    },\r\n    showLinkMenu(attrs) {\r\n      this.linkUrl = attrs.href\r\n      this.linkMenuIsActive = true\r\n      this.$nextTick(() => {\r\n        this.$refs.linkInput.focus()\r\n      })\r\n    },\r\n    hideLinkMenu() {\r\n      this.linkUrl = null\r\n      this.linkMenuIsActive = false\r\n    },\r\n    setLinkUrl(command, url) {\r\n      command({ href: url })\r\n      this.hideLinkMenu()\r\n    },\r\n    onCategoryChange(selectedcategory){\r\n      this.categories.forEach(category => {\r\n        if(category.id==selectedcategory){\r\n          this.categoryimg=category.image\r\n      console.log(category.image)\r\n        }\r\n      });\r\n    }\r\n  },\r\n  mounted: function () {\r\n    this.store_id = this.$route.params.id\r\n    this.fetchStore(this.store_id).then(data => {\r\n      this.store = data\r\n      this.editor.setContent(data.description)\r\n      for (let category in data.categories){\r\n        this.categoryoptions.push({\r\n          key: category,\r\n          text: category,\r\n          value: data.categories[category],\r\n        })\r\n      }\r\n      this.maincategory = data.main_category.display\r\n      for (let url in data.images){\r\n        let imgurl = \"https://my.zuly.it/api/stores/\"+this.$route.params.id+\"/images/\"+data.images[url]\r\n        if(data.images[url]!=null){\r\n          if(url==0)\r\n            this.imgurls.push({\r\n              url:imgurl,\r\n              classname:\"carousel-item active\"\r\n            })\r\n          else\r\n            this.imgurls.push({\r\n              url:imgurl,\r\n              classname:\"carousel-item\"\r\n            })\r\n        }\r\n      }\r\n      console.log(this.imgurls)\r\n    })\r\n    this.fetchCategory().then(data=>{\r\n      this.categories=data\r\n    })\r\n  }\r\n}\r\n\r\n</script>"],"sourceRoot":"src/pages"}]}